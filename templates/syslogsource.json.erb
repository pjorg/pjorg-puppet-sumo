{
    "api.version": "v1",
    "source": {
<% unless @description.nil?             %>        "description": "<%= @description %>",
<% end
unless @category.nil?                   %>        "category": "<%= @category %>",
<% end
unless @hostname.nil?                   %>        "hostName": "<%= @hostname %>",
<% end
unless @timezone.nil?                   %>        "timeZone": "<%= @timezone %>",
<% end
                                        %>        "sourceType": "<%= @sourcetype %>",
<% unless @automaticdateparsing.nil?    %>        "automaticDateParsing": "<%= @automaticdateparsing.to_s.upcase %>",
<% end
unless @multilineprocessingenabled.nil? %>        "multilineProcessingEnabled": "<%= @multilineprocessingenabled.to_s.upcase %>",
<% end
unless @useautolinematching.nil?        %>        "useAutolineMatching": "<%= @useautolinematching.to_s.upcase %>",
<% end
unless @manualprefixregexp.nil?         %>        "manualPrefixRegexp": "<%= @manualprefixregexp %>",
<% end
unless @forcetimezone.nil?              %>        "forceTimeZone": "<%= @forcetimezone.to_s.upcase %>",
<% end
unless @defaultdateformat.nil?          %>        "defaultDateFormat": "<%= @defaultdateformat %>",
<% end
unless @filters.nil?                    %>        "filters": [<%= @filters.map{ |filter| "\"#{filter}\"" }.join(',') %>],
<% end
                                        %>        "protocol": "<%= @protocol.upcase %>",
<%                                      %>        "port": <%= @port %>,
<%                                      %>        "name": "<%= @sourcename %>"
    }
}
